{
  "compilerOptions": {
    "target": "es2020",
    "module": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "verbatimModuleSyntax": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "declarationDir": "../fk-utils/esm/types",
    "outDir": "../fk-utils/esm"
  }
}
